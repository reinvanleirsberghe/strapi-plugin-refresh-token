{
  "name": "@revale/strapi-plugin-refresh-token",
  "description": "Plugin for Strapi 5 to provide refresh token functionality.",
  "license": "MIT",
  "author": "Rein Van Leirsberghe",
  "repository": {
    "url": "git+https://github.com/reinvanleirsberghe/strapi-plugin-refresh-token.git",
    "type": "git",
    "directory": "."
  },
  "version": "0.0.6",
  "keywords": [],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test": "jest",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "react-intl": "^7.0.1"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.4.2",
    "@strapi/typescript-utils": "^5.4.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1",
    "styled-components": "^6.1.13",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "refresh-token",
    "displayName": "Strapi Plugin Refresh Token",
    "description": "Plugin for Strapi 5 to provide refresh token functionality. "
  },
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/redon2/strapi-plugin-refresh-token/issues"
  },
  "homepage": "https://github.com/redon2/strapi-plugin-refresh-token#readme"
}
